<Window x:Class="Asterisk_Manager.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="350" Width="525">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#dbe586" Offset="0"/>
            <GradientStop Color="#95b038" Offset="0.992"/>
            <GradientStop Color="#dbe586" Offset="0.185"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid>
        <ContentControl prism:RegionManager.RegionName="MainView" />
    </Grid>
</Window>
